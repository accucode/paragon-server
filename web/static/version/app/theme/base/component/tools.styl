/*******************************************************************
 ** flexbox :: parent
 **
 ** These styles apply to the PARENT, the flexbox itself.
 ** http://css-tricks.com/snippets/css/a-guide-to-flexbox/
 *******************************************************************/


/* the style (BLOCK) and direction (row/column) */
.flexRow            { display: flex; flex-direction: row;              }
.flexRowReverse     { display: flex; flex-direction: row-reverse;      }
.flexColumn         { display: flex; flex-direction: column;           }
.flexColumnReverse  { display: flex; flex-direction: column-reverse;   }


/* the style (INLINE) and direction (row/column) */
.flexInlineRow            { display: inline-flex; flex-direction: row;              }
.flexInlineRowReverse     { display: inline-flex; flex-direction: row-reverse;      }
.flexInlineColumn         { display: inline-flex; flex-direction: column;           }
.flexInlineColumnReverse  { display: inline-flex; flex-direction: column-reverse;   }


/* distribute items along MAIN axis, default is START */
.flexAlignStart         { justify-content: flex-start;      }
.flexAlignEnd           { justify-content: flex-end         }
.flexAlignCenter        { justify-content: center;          }
.flexAlignSpaced        { justify-content: space-between;   }
.flexAlignSpacedAround  { justify-content: space-around;    }


/* distribute items along the CROSS axis, default is START */
.flexCrossAlignStretch  { align-items: stretch;     }
.flexCrossAlignStart    { align-items: flex-start;  }
.flexCrossAlignEnd      { align-items: flex-end;    }
.flexCrossAlignCenter   { align-items: center;      }
.flexCrossAlignBaseline { align-items: baseline;    }


/* allow wrapping; default is NONE */
.flexNoWrap         { flex-wrap: none;          }
.flexWrap           { flex-wrap: wrap;          }
.flexWrapReverse    { flex-wrap: wrap-reverse;  }


/* distribute lines along the CROSS axis, ONLY if wrapping is enabled */
.flexWrapAlignStretch       { align-content: stretch;       }
.flexWrapAlignStart         { align-content: flex-start;    }
.flexWrapAlignEnd           { align-content: flex-end       }
.flexWrapAlignCenter        { align-content: center;        }
.flexWrapAlignSpaced        { align-content: space-between; }
.flexWrapAlignSpacedAround  { align-content: space-around;  }


 /*******************************************************************
 ** flexbox :: child
 **
 ** This styles should be applied to the CHILDREN of a flexbox.
 ** http://css-tricks.com/snippets/css/a-guide-to-flexbox/
 *******************************************************************/


/* override the item alignment on the CROSS axis, default is AUTO */
.flexChildCrossOverrideAuto      { align-self: auto;         }
.flexChildCrossOverrideStart     { align-self: flex-start;   }
.flexChildCrossOverrideEnd       { align-self: flex-end;     }
.flexChildCrossOverrideCenter    { align-self: center;       }
.flexChildCrossOverrideBaseline  { align-self: baseline;     }
.flexChildCrossOverrideStretch   { align-self: stretch;      }


.flexChildBasis0 { flex-basis: 0px; }


/* can the item grow; default is NO GROW */
.flexChildGrow      { flex-grow: 1; }
.flexChildNoGrow    { flex-grow: 0; }


/* can the item shrink, default is SHRINK */
.flexChildShrink    { flex-shrink: 1; }
.flexChildNoShrink  { flex-shrink: 0; }


/* convenience for grow and shrink */
.flexChildStatic    { flex-grow:0; flex-shrink:0; }
.flexChildFiller    { flex-grow:1; flex-shrink:1; }
.flexChildFiller0   { flex-grow:1; flex-shrink:1; flex-basis: 0px; }


.flexChildPhiLarge  { flex-grow:8; flex-shrink:8; }
.flexChildPhiSmall  { flex-grow:5; flex-shrink:5; }


.flexChildPhiLarge0 { flex-grow:8; flex-shrink:8; flex-basis: 0px; }
.flexChildPhiSmall0 { flex-grow:5; flex-shrink:5; flex-basis: 0px; }


.flexChildSmall     { flex-grow:3; flex-shrink:5; }
.flexChildSmall0    { flex-grow:3; flex-shrink:5; flex-basis: 0px;}


//******************************
//** flex child :: parts
//******************************


.flexChildParts10   { flex-grow:10;  flex-shrink:10;  flex-basis: 0px; }
.flexChildParts20   { flex-grow:20;  flex-shrink:20;  flex-basis: 0px; }
.flexChildParts30   { flex-grow:30;  flex-shrink:30;  flex-basis: 0px; }
.flexChildParts40   { flex-grow:40;  flex-shrink:40;  flex-basis: 0px; }
.flexChildParts50   { flex-grow:50;  flex-shrink:50;  flex-basis: 0px; }
.flexChildParts60   { flex-grow:60;  flex-shrink:60;  flex-basis: 0px; }
.flexChildParts70   { flex-grow:70;  flex-shrink:70;  flex-basis: 0px; }
.flexChildParts80   { flex-grow:80;  flex-shrink:80;  flex-basis: 0px; }
.flexChildParts90   { flex-grow:90;  flex-shrink:90;  flex-basis: 0px; }
.flexChildParts100  { flex-grow:100; flex-shrink:100; flex-basis: 0px; }


//******************************
